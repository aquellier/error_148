<div class="container">
  <div class="row">
      <% @challenges.each do |challenge| %>
        <li class="list_challenges">
            <h3><strong><%= challenge.name%></strong></h3>
            <p><%= challenge.language.name %></p>
            <p><%= challenge.description %></p>
            <br>
            <p><%= link_to "See page", challenge.link %></p>
            <br>
            <p><%= link_to "See github code", challenge.link_git %></p>
        </li>
      <% end %>
  </div>
</div>


<%= simple_form_for(@challenge) do |f| %>
  <%= f.input :name %>
  <%= f.input :description%>
  <%= f.input :link %>
  <%= f.input :link_git %>

  <div class="flex-side">
    <%= f.button :submit, class: "flex-item btn btn-primary"%>
    <%= link_to 'Go back to list of errors', errorss_path, class: 'flex-item btn btn-danger' %>
  </div>
<% end %>
